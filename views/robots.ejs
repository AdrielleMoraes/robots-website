<%- include("partials/header.ejs"); %>

<div class="col-lg-12">
    <h3>Our Most Popular Robots</h3>
    </div>
<div class="card-columns">

    <%robots.forEach(robot => {%>
        <div class = "card">
            
            <img class="img-fluid" src = "<%=robot.image%>"> 
            <div class="card-body">
                <h4><%= robot.name %> </h4>
            </div>

        </div>
    <%}); %> 
</div>
    

<%- include("partials/footer.ejs"); %>